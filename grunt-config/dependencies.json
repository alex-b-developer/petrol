{
  "options": {
    "report": "true",
    "clean": "true",
    "destPrefix": "<%= path.source %>/assets/lib"
  },
  "bootstrap": {
    "files":{
      "bootstrap/less" : "bootstrap/less",
      "bootstrap/dist" : "bootstrap/dist"
    }
  },
  "jquery":{
    "files":{
      "jquery" : "jquery/dist"
    }
  },
  "open-sans":{
    "files":{
      "open-sans" : "open-sans/fonts"
    }
  },
  "lato":{
    "files":{
      "lato/css" : "lato/css",
      "lato/font" : "lato/font"
    }
  },
  "linea-io":{
    "files":{
      "linea-io" : "linea-io/linea.css",
      "linea-io/fonts" : "linea-io/fonts"
    }
  }
}